local function tweento(x,xx,y,yy,frame,time,destroyaftertween)
	game:GetService("TweenService"):Create(frame,TweenInfo.new(time),{Size = UDim2.new(x,xx,y,yy)}):Play()
	task.wait(time)
	destroyaftertween = destroyaftertween or false
	if destroyaftertween then
		frame:Destroy()
	end
end
