					if input.UserInputType == Enum.UserInputType.MouseButton1 then
						TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(red,green,blue)}):Play()
					end
				end)
			end
			coroutine.wrap(VCLMAW_script)()
			infohover = false
			connection1 = InfoIcon.MouseEnter:Connect(function()
				infohover = true
			end)
			connection2 = InfoIcon.MouseLeave:Connect(function()
				infohover = false
			end)
			connection3 = Button.InputBegan:Connect(function(i)
				if i.UserInputType == Enum.UserInputType.MouseButton1 and not infohover then
					callback()
				end
			end)
			connection4 = InfoIcon.InputBegan:Connect(function(i)
				if i.UserInputType == Enum.UserInputType.MouseButton1 then
					Library:notification("Button Info",desc,5)
				end
			end)
			BTF = {}
