			local function ZMRKW_script()
				local script = Instance.new('LocalScript', SliderTop)
				local coloradd = 5
				local time = 0.1
				local tweeninfoV = TweenInfo.new(time)
				local ts = game:GetService("TweenService")
				local sp = script.Parent
				local red = sp.BackgroundColor3.R*255
				local green = sp.BackgroundColor3.G*255
				local blue = sp.BackgroundColor3.B*255
				local redP = red + coloradd
				local greenP = green + coloradd
				local blueP = green + coloradd
				local redC = redP*2
				local greenC = greenP*2
				local blueC = blueP*2
				sp.MouseEnter:Connect(function()
					ts:Create(sp,tweeninfoV,{BackgroundColor3 = Color3.fromRGB(redP,greenP,blueP)}):Play()
					task.wait(time)
					sp.BackgroundColor3 = Color3.fromRGB(redP,greenP,blueP)
				end)
				sp.MouseLeave:Connect(function()
					ts:Create(sp,tweeninfoV,{BackgroundColor3 = Color3.fromRGB(red,green,blue)})
					task.wait(time)
					sp.BackgroundColor3 = Color3.fromRGB(red,green,blue)
				end)
				sp.InputBegan:Connect(function(i)
					if i.UserInputType == Enum.UserInputType.MouseButton1 then
						ts:Create(sp,tweeninfoV,{BackgroundColor3 = Color3.fromRGB(redC,greenC,blueC)}):Play()
						task.wait(time)
						sp.BackgroundColor3 = Color3.fromRGB(redC,greenC,blueC)
					end
				end)
				sp.InputEnded:Connect(function(i)
					if i.UserInputType == Enum.UserInputType.MouseButton1 then
						ts:Create(sp,tweeninfoV,{BackgroundColor3 = Color3.fromRGB(red,green,blue)}):Play()
						task.wait(time)
						sp.BackgroundColor3 = Color3.fromRGB(red,green,blue)
					end
				end)
			end
			coroutine.wrap(ZMRKW_script)()
			local function val()
				percentage = math.clamp((plr:GetMouse().X-SliderBack.AbsolutePosition.X)/SliderBack.AbsoluteSize.X,0,1)
				value = (math.floor((((max-min)*(percentage))+min)/increment))*increment
				TextLabel.Text = (value.."/"..max)
				SliderTop:TweenSize(UDim2.new(percentage,0,1,0),Enum.EasingDirection.Out,Enum.EasingStyle.Linear,0.1)
				return value,callback(value)
			end
			dragging = false
			connection1 = SliderBack.InputBegan:Connect(function(i)
				if i.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging = true
					while task.wait() do
						if dragging then
							vall = val()
						else
							break
						end
					end
				end
			end)
			connection2 = SliderBack.InputEnded:Connect(function(i)
				if i.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging = false
				end
			end)
			connection3 = InfoIcon.InputBegan:Connect(function(i)
				if i.UserInputType == Enum.UserInputType.MouseButton1 then
					Library:notification("Slider Info",desc,5)
				end
			end)
			--funcs
			SDF = {}
			function SDF:ChangeName(text)
				Label.Text = text
			end
			function SDF:ChangeDescription(descr)
				desc = descr
			end
			function SDF:ChangeIncrement(inc)
				increment = inc
				TextLabel.Text = ((math.floor(vall/increment))*increment).."/"..max
			end
			function SDF:ChangeInterval(mn,mx)
				min = mn
				max = mx
				TextLabel.Text = ((math.floor(vall/increment))*increment).."/"..max
			end
			function SDF:ChangeValue(value)
				TextLabel.Text = value.."/"..max
				callback(value)
			end
			function SDF:ChangeCallback(funcc)
				callback = funcc
			end
			function SDF:Break()
				connection1:Disconnect()
				Slider:Destroy()
			end
			return SDF
		end
		function tab:keybind(name,desc,cb,callback)
			local Keybind = Instance.new("Frame")
			local UICorner = Instance.new("UICorner")
			local Label = Instance.new("TextLabel")
			local InfoIcon = Instance.new("ImageLabel")
			local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
			local KBIcon = Instance.new("ImageLabel")
			local UIAspectRatioConstraint_2 = Instance.new("UIAspectRatioConstraint")
			local KeyBindBox = Instance.new("TextBox")
			Keybind.Name = guid()
			Keybind.Parent = tabf
			Keybind.BackgroundColor3 = Color3.fromRGB(88, 88, 88)
			Keybind.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Keybind.BorderSizePixel = 0
			Keybind.Position = UDim2.new(0.0888186693, 0, 0.16560261, 0)
			Keybind.Size = UDim2.new(0, 266, 0, 30)
			Keybind.ZIndex = 5
			UICorner.Name = guid()
			UICorner.CornerRadius = UDim.new(1, 0)
			UICorner.Parent = Keybind
			Label.Name = guid()
			Label.Parent = Keybind
			Label.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Label.BackgroundTransparency = 1.000
			Label.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Label.BorderSizePixel = 0
			Label.Position = UDim2.new(0.127819553, 0, 0, 0)
			Label.Size = UDim2.new(0.492481202, 0, 1, 0)
			Label.Font = Enum.Font.SourceSans
			Label.Text = name or "Keybind"
			Label.TextColor3 = Color3.fromRGB(0, 0, 0)
			Label.TextSize = 14.000
			Label.TextWrapped = true
			Label.TextXAlignment = Enum.TextXAlignment.Left
			InfoIcon.Name = guid()
			InfoIcon.Parent = Keybind
			InfoIcon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			InfoIcon.BackgroundTransparency = 1.000
			InfoIcon.BorderColor3 = Color3.fromRGB(0, 0, 0)
			InfoIcon.BorderSizePixel = 0
			InfoIcon.Position = UDim2.new(0.936090231, 0, 0.200000018, 0)
			InfoIcon.Size = UDim2.new(0.0639097765, 0, 0.666666687, 0)
			InfoIcon.ZIndex = 55
			InfoIcon.Image = "rbxassetid://13012250972"
			InfoIcon.ImageTransparency = 0.500
			UIAspectRatioConstraint.Name = guid()
			UIAspectRatioConstraint.Parent = InfoIcon
			KBIcon.Name = guid()
			KBIcon.Parent = Keybind
			KBIcon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			KBIcon.BackgroundTransparency = 1.000
			KBIcon.BorderColor3 = Color3.fromRGB(0, 0, 0)
			KBIcon.BorderSizePixel = 0
			KBIcon.Position = UDim2.new(0.0338345841, 0, 0.200000003, 0)
			KBIcon.Size = UDim2.new(0.19924812, 0, 0.600000024, 0)
			KBIcon.ZIndex = 55
			KBIcon.Image = "rbxassetid://4893250303"
			KBIcon.ImageColor3 = Color3.fromRGB(132, 132, 132)
			UIAspectRatioConstraint_2.Name = guid()
			UIAspectRatioConstraint_2.Parent = KBIcon
			KeyBindBox.Name = guid()
			KeyBindBox.Parent = Keybind
			KeyBindBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			KeyBindBox.BackgroundTransparency = 1.000
			KeyBindBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
			KeyBindBox.BorderSizePixel = 0
			KeyBindBox.Position = UDim2.new(0.0338345878, 0, 0.200000003, 0)
			KeyBindBox.Size = UDim2.new(0.0676691756, 0, 0.566666663, 0)
			KeyBindBox.ZIndex = 999
			KeyBindBox.Font = Enum.Font.SourceSans
			KeyBindBox.Text = (string.split(tostring(cb),"."))[3] or "RightBracket"
			KeyBindBox.TextColor3 = Color3.fromRGB(0, 0, 0)
			KeyBindBox.TextSize = 14.000
			key = cb or Enum.KeyCode.RightBracket
			focused = false
			local function XRQOZBG_script()
				local script = Instance.new('LocalScript', KeyBindBox)
				script.Parent.Changed:Connect(function()
					script.Parent.Text = string.upper(string.sub(script.Parent.Text,#script.Parent.Text,#script.Parent.Text))
				end)
			end
			coroutine.wrap(XRQOZBG_script)()
			connection1 = KeyBindBox.Focused:Connect(function()
				focused = true
			end)
			connection2 = KeyBindBox.FocusLost:Connect(function()
				focused = false
			end)
			connection3 = game:GetService("UserInputService").InputBegan:Connect(function(i)
				if i.KeyCode ~= Enum.KeyCode.Unknown then
					if focused then
						key = i.KeyCode
					elseif not focused and i.KeyCode == key then
						callback(key,(string.split(tostring(key),"."))[3])
					end
				end
			end)
			InfoIcon.InputBegan:Connect(function(i)
				if i.UserInputType == Enum.UserInputType.MouseButton1 then
					Library:notification("Keybind Info",desc,5)
				end
			end)
			--update funcs
			KBF = {}
			function KBF:ChangeName(name)
				Label.Text = name
			end
			function KBF:ChangeBind(bindd)
				key = bindd
				KeyBindBox.Text = (string.split(tostring(key),"."))[3]
			end
			function KBF:ChangeDescription(descr)
				desc = descr
			end
			function KBF:ChangeCallback(funcc)
				callback = funcc
			end
			function KBF:Break()
				connection1:Disconnect()
				connection2:Disconnect()
				connection3:Disconnect()
				Keybind:Destroy()
			end
			return KBF
		end
		return tab
	end
	return window
end
return Library,help
