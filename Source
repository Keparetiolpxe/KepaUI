local Library = {}
local help = {}
local plr = game:GetService("Players").LocalPlayer

function help:cnnil()
	if plr.Character and plr.Character:FindFirstChild("Head") or plr.Character:FindFirstChild("HumanoidRootPart") or plr.Character.PrimaryPart ~= nil then
		return true
	else
		return false
	end
end
function help:tpto(x,y,z,back,time)
	if help:cnnil() then
		back = back or false
		time = time or 0
		lp = plr.Character.HumanoidRootPart.CFrame
		plr.Character.HumanoidRootPart.CFrame = CFrame.new(x,y,z,1,0,0,0,1,0,0,0,1)
		if back == true then
			task.wait(time)
			plr.Character.HumanoidRootPart.CFrame = lp
		end
	end
end
function help:touch(part,from,time)
	from  = from or plr.Character.HumanoidRootPart
	time = time or 0
	if not part then return end
	if help:cnnil() then
		lp = part.CFrame
		part.CFrame = from.CFrame
		task.wait(time)
		part.CFrame = lp
	end
end
function help:topart(part,from,back,time)
	from  = from or plr.Character.HumanoidRootPart
	time = time or 0
	back = back or false
	if not part then return end
	if help:cnnil() then
		if back then
			from.CFrame = part.CFrame
		else
			lp = from.CFrame
			from.CFrame = part.CFrame
			task.wait(time)
			from.CFrame = lp
		end
	end
end
function help:suicide()
	--copied from inf yield
	local char = plr.Character
	if char:FindFirstChildOfClass("Humanoid") then char:FindFirstChildOfClass("Humanoid"):ChangeState(15) end
	char:ClearAllChildren()
	local newChar = Instance.new("Model")
	newChar.Parent = workspace
	plr.Character = newChar
	wait()
	plr.Character = char
	newChar:Destroy()
end
function help:clip(text)
	local board = setclipboard or toclipboard
	if board then
		board(text)
	end
end
local safepath = game:GetService("CoreGui")
local function guid()
	return game:GetService("HttpService"):GenerateGUID(false)
end
