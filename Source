		Ignore.TextSize = 14.000
		UICorner_3.Name = guid()
		UICorner_3.CornerRadius = UDim.new(0, 5)
		UICorner_3.Parent = Ignore
		onagree = onagree or function() end
		Ok.MouseButton1Click:Connect(function()
			onagree()
			tweento(0,0,0,0,Notification,0.2,true)
		end)
		Ignore.MouseButton1Click:Connect(function()
			tweento(0,0,0,0,Notification,0.2,true)
		end)
		task.wait(selfdtime)
		tweento(0,0,0,0,Notification,0.2,true)
	end
	local function XESUAF_script()
		local script = Instance.new('LocalScript', NameFrame)
		script.Name = game:GetService("HttpService"):GenerateGUID()
		local input = game:GetService("UserInputService")
		local TS = game:GetService("TweenService")
		local function EnabledDrag(frame)
			frame = frame or script.Parent
			local dragging = false
			local dragInput, mousePos, framePos
			frame.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging = true
					mousePos = input.Position
					framePos = frame.Position
					input.Changed:Connect(function()
						if input.UserInputState == Enum.UserInputState.End then
							dragging = false
						end
					end)
				end
			end)
			frame.InputChanged:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseMovement then
					dragInput = input
				end
			end)
			input.InputChanged:Connect(function(input)
				if input == dragInput and dragging then
					local delta = input.Position - mousePos
					local destination = UDim2.new(framePos.X.Scale, framePos.X.Offset + delta.X, framePos.Y.Scale, framePos.Y.Offset + delta.Y)
					TS:Create(frame,TweenInfo.new(0.1),{Position = destination}):Play()
				end
			end)
		end
		
		EnabledDrag(script.Parent)
	end
	coroutine.wrap(XESUAF_script)()
	local function HMIXU_script()
		local script = Instance.new('LocalScript', CopyButtonCC)

		local coloradd = 0.1
		local tweentime = 0.1
		
		local TS = game:GetService("TweenService")
		
		local red = script.Parent.BackgroundColor.r
		local green = script.Parent.BackgroundColor.g
		local blue = script.Parent.BackgroundColor.b
		
		local redP = red+coloradd
		local greenP = green+coloradd
		local blueP = blue+coloradd
		
		local redC = red+coloradd*2
		local greenC = green+coloradd*2
		local blueC = blue+coloradd*2
		script.Parent.MouseEnter:Connect(function()
			TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(redP,greenP,blueP)}):Play()
		end)
		script.Parent.MouseLeave:Connect(function()
			TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(red,green,blue)}):Play()
		end)
		script.Parent.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(redC,greenC,blueC)}):Play()
			end
		end)
		script.Parent.InputEnded:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(red,green,blue)}):Play()
			end
		end)
	end
	coroutine.wrap(HMIXU_script)()
