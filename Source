			Drop.AutomaticCanvasSize = Enum.AutomaticSize.Y
			UIListLayout.Name = guid()
			UIListLayout.Parent = Drop
			UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout.Padding = UDim.new(0, 3)
			local Option = Instance.new("TextLabel")
			Option.Name = guid()
			Option.Parent = Dropdown
			Option.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Option.BackgroundTransparency = 1.000
			Option.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Option.BorderSizePixel = 0
			Option.Position = UDim2.new(0.774436116, 0, 0, 0)
			Option.Size = UDim2.new(0.142857164, 0, 0, 30)
			Option.Font = Enum.Font.SourceSans
			Option.Text = currentoption or "---"
			Option.TextColor3 = Color3.fromRGB(0, 0, 0)
			Option.TextSize = 14.000
			Option.TextWrapped = true
			local function UFJOBSX_script()
				local script = Instance.new('LocalScript', DDIcon)
				local coloradd = 0.35
				local tweentime = 0.1
				local TS = game:GetService("TweenService")
				local red = script.Parent.ImageColor3.R
				local green = script.Parent.ImageColor3.G
				local blue = script.Parent.ImageColor3.B
				local redP = red+coloradd
				local greenP = green+coloradd
				local blueP = blue+coloradd
				local redC = red+coloradd*2
				local greenC = green+coloradd*2
				local blueC = blue+coloradd*2
				if redP>1 then
					redP = 1
				end
				script.Parent.MouseEnter:Connect(function()
					TS:Create(script.Parent,TweenInfo.new(tweentime),{ImageColor3 = Color3.new(redP,greenP,blueP)}):Play()
				end)
				script.Parent.MouseLeave:Connect(function()
					TS:Create(script.Parent,TweenInfo.new(tweentime),{ImageColor3 = Color3.new(red,green,blue)}):Play()
				end)
				script.Parent.InputBegan:Connect(function(input)
					if input.UserInputType == Enum.UserInputType.MouseButton1 then
						TS:Create(script.Parent,TweenInfo.new(tweentime),{ImageColor3 = Color3.new(redC,greenC,blueC)}):Play()
					end
				end)
				script.Parent.InputEnded:Connect(function(input)
					if input.UserInputType == Enum.UserInputType.MouseButton1 then
						TS:Create(script.Parent,TweenInfo.new(tweentime),{ImageColor3 = Color3.new(red,green,blue)}):Play()
					end
				end)
			end
			coroutine.wrap(UFJOBSX_script)()
			
			--Getting options
			local function optionget()
				for i,v in pairs(options) do
					local Button = Instance.new("Frame")
					local TextLabel = Instance.new("TextLabel")
					local UICorner = Instance.new("UICorner")
					Button.Name = guid()
					Button.Parent = Drop
					Button.BackgroundColor3 = Color3.fromRGB(112, 112, 112)
					Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
					Button.BorderSizePixel = 0
					Button.Size = UDim2.new(0, 266, 0, 24)
					Button.ZIndex = 5
					TextLabel.Name = guid()
					TextLabel.Parent = Button
					TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					TextLabel.BackgroundTransparency = 1.000
					TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
					TextLabel.BorderSizePixel = 0
					TextLabel.Position = UDim2.new(0.0639097765, 0, 0, 0)
					TextLabel.Size = UDim2.new(0.815789402, 0, 1, 0)
					TextLabel.Font = Enum.Font.SourceSans
					TextLabel.Text = v
					TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
					TextLabel.TextSize = 14.000
					TextLabel.TextWrapped = true
					TextLabel.TextXAlignment = Enum.TextXAlignment.Left
					UICorner.Name = guid()
					UICorner.CornerRadius = UDim.new(1, 0)
					UICorner.Parent = Button
					local function OZBT_script() -- Button.ColorRider 
						local script = Instance.new('LocalScript', Button)
						local coloradd = 0.1
						local tweentime = 0.1
						local TS = game:GetService("TweenService")
						local red = script.Parent.BackgroundColor.r
						local green = script.Parent.BackgroundColor.g
						local blue = script.Parent.BackgroundColor.b
						local redP = red+coloradd
						local greenP = green+coloradd
						local blueP = blue+coloradd
						local redC = red+coloradd*2
						local greenC = green+coloradd*2
						local blueC = blue+coloradd*2
						script.Parent.MouseEnter:Connect(function()
							TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(redP,greenP,blueP)}):Play()
						end)
						script.Parent.MouseLeave:Connect(function()
							TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(red,green,blue)}):Play()
						end)
						script.Parent.InputBegan:Connect(function(input)
							if input.UserInputType == Enum.UserInputType.MouseButton1 then
								TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(redC,greenC,blueC)}):Play()
							end
						end)
						script.Parent.InputEnded:Connect(function(input)
							if input.UserInputType == Enum.UserInputType.MouseButton1 then
								TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(red,green,blue)}):Play()
							end
						end)
					end
					coroutine.wrap(OZBT_script)()
					Button.InputBegan:Connect(function(i)
						if i.UserInputType == Enum.UserInputType.MouseButton1 then
							Drop.Visible = false
							game:GetService("TweenService"):Create(DDIcon,TweenInfo.new(0.5),{Rotation = -90}):Play()
							tweento(0,266,0,30,Dropdown,0.5)
							Option.Text = v
							return callback(v)
						end
					end)
				end
			end
			optionget()
			connection2 = DDIcon.InputBegan:Connect(function(i)
				if i.UserInputType == Enum.UserInputType.MouseButton1 then
					if Drop.Visible then
						Drop.Visible = false
						game:GetService("TweenService"):Create(DDIcon,TweenInfo.new(0.5),{Rotation = -90}):Play()
						tweento(0,266,0,30,Dropdown,0.5)
					else
						Drop.Visible = true
						game:GetService("TweenService"):Create(DDIcon,TweenInfo.new(0.5),{Rotation = 0}):Play()
						tweento(0,266,0,155,Dropdown,0.5)
					end
				end
			end)
			connection1 = InfoIcon.InputBegan:Connect(function(i)
				if i.UserInputType == Enum.UserInputType.MouseButton1 then
					Library:notification("Dropdown Info",desc,5)
				end
			end)
			DDF = {}
			function DDF:ChangeName(text)
				Label.Text = text
			end
			function DDF:ChangeDescription(descc)
				desc = descc
			end
			function DDF:ChangeCallback(callbackk)
				callback = callbackk
			end
			function DDF:ChangeList(newlist)
				for i,v in pairs(Drop:GetChildren()) do
					if v:IsA("Frame") then
						v:Destroy()
					end
				end
				options = newlist
				optionget()
			end
			function DDF:AddList(addinglist)
				options = addinglist
				optionget()
			end
			function DDF:Select(optionn)
				callback(optionn)
			end
			function DDF:Break()
				connection1:Disconnect()
				connection2:Disconnect()
				Dropdown:Destroy()
			end
			return DDF
		end
