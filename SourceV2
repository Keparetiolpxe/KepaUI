			Label.Size = UDim2.new(0.492000014, 0, 0, 30)
			Label.Font = Enum.Font.SourceSans
			Label.Text = name
			Label.TextColor3 = Color3.fromRGB(0, 0, 0)
			Label.TextSize = 14.000
			Label.TextWrapped = true
			Label.TextXAlignment = Enum.TextXAlignment.Left
			InfoIcon.Name = guid()
			InfoIcon.Parent = Dropdown
			InfoIcon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			InfoIcon.BackgroundTransparency = 1.000
			InfoIcon.BorderColor3 = Color3.fromRGB(0, 0, 0)
			InfoIcon.BorderSizePixel = 0
			InfoIcon.Position = UDim2.new(0.93599999, 0, 0, 6)
			InfoIcon.Size = UDim2.new(0, 17, 0, 17)
			InfoIcon.ZIndex = 55
			InfoIcon.Image = "rbxassetid://13012250972"
			InfoIcon.ImageTransparency = 0.500
			UIAspectRatioConstraint.Name = guid()
			UIAspectRatioConstraint.Parent = InfoIcon
			DDIcon.Name = guid()
			DDIcon.Parent = Dropdown
			DDIcon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			DDIcon.BackgroundTransparency = 1.000
			DDIcon.BorderColor3 = Color3.fromRGB(0, 0, 0)
			DDIcon.BorderSizePixel = 0
			DDIcon.Position = UDim2.new(0.0340000018, 0, 0, 6)
			DDIcon.Rotation = -90.000
			DDIcon.Size = UDim2.new(0, 18, 0, 18)
			DDIcon.ZIndex = 55
			DDIcon.Image = "rbxassetid://5279719038"
			DDIcon.ImageColor3 = Color3.fromRGB(132, 132, 132)
			UIAspectRatioConstraint_2.Name = guid()
			UIAspectRatioConstraint_2.Parent = DDIcon
			Drop.Name = guid()
			Drop.Parent = Dropdown
			Drop.Active = true
			Drop.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
