	BindLabel.Text = "[Enum.KeyCode.RightBracket]"
	BindLabel.TextColor3 = Color3.fromRGB(59, 59, 59)
	BindLabel.TextSize = 14.000
	BindLabel.TextXAlignment = Enum.TextXAlignment.Right
	BindLabel.InputBegan:Connect(function(i)
		if i.UserInputType == Enum.UserInputType.MouseButton1 then
			BindLabel.TextColor3 = Color3.fromRGB(70,70,70)
			local disconnect = game:GetService("UserInputService").InputBegan:Connect(function(i)
				if i.KeyCode == Enum.KeyCode.Unknown then return end
				BindLabel.Text = tostring(i.KeyCode)
				showbind = i.KeyCode
				disconnect:Disconnect()
			end)
		end
	end)
