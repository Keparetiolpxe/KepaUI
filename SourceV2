			local function DGCXEFR_script()
				local script = Instance.new('LocalScript', DestroyUI)
				local coloradd = 0.1
				local tweentime = 0.1
				local TS = game:GetService("TweenService")
				local red = script.Parent.BackgroundColor.r
				local green = script.Parent.BackgroundColor.g
				local blue = script.Parent.BackgroundColor.b
				local redP = red+coloradd
				local greenP = green+coloradd
				local blueP = blue+coloradd
				local redC = red+coloradd*2
				local greenC = green+coloradd*2
				local blueC = blue+coloradd*2
				script.Parent.MouseEnter:Connect(function()
					TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(redP,greenP,blueP)}):Play()
				end)
				script.Parent.MouseLeave:Connect(function()
					TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(red,green,blue)}):Play()
				end)
				script.Parent.InputBegan:Connect(function(input)
					if input.UserInputType == Enum.UserInputType.MouseButton1 then
						TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(redC,greenC,blueC)}):Play()
					end
				end)
				script.Parent.InputEnded:Connect(function(input)
					if input.UserInputType == Enum.UserInputType.MouseButton1 then
						TS:Create(script.Parent,TweenInfo.new(tweentime),{BackgroundColor3 = Color3.new(red,green,blue)}):Play()
					end
				end)
			end
			coroutine.wrap(DGCXEFR_script)()
			connection1 = DestroyUI.InputBegan:Connect(function(i)
				if i.UserInputType == Enum.UserInputType.MouseButton1 then
					tweento(0,0,0,0,MainFrame,0.2,true)
					for i,v in pairs(DisconnectButtons) do
						v:Break()
					end
				end
			end)
			UBF = {}
			function UBF:Break()
				connection1:Disconnect()
			end
			function UBF:Fire()
				tweento(0,0,0,0,MainFrame,0.2,true)
			end
			table.insert(DisconnectButtons,UBF)
			return UBF
		end
