			BTF = {}
			function BTF:ChangeName(text)
				TextLabel = text
			end
			function BTF:ChangeDescription(text)
				desc = text
			end
			function BTF:Fire()
				callback()
			end
			function BTF:ChangeCallback(call)
				callback = call
			end
			function BTF:Break()
				connection1:Disconnect()
				connection2:Disconnect()
				connection3:Disconnect()
				connection4:Disconnect()
				Button:Destroy()
			end
			table.insert(DisconnectButtons,BTF)
			return BTF
		end
		function tab:toggle(name,desc,currentval,callback)
			name = name or "Toggle"
			desc = desc or "No description"
			callback = callback or function() end
			if desc == "" then
				desc = "No description"
			end
			local Toggle = Instance.new("Frame")
