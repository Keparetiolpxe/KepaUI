			if i.UserInputType == Enum.UserInputType.MouseButton1 then
				for i,bb in pairs(Tabs:GetChildren()) do
					if bb:IsA("Frame") then
						game:GetService("TweenService"):Create(bb,TweenInfo.new(0.2),{BackgroundTransparency = 0.95}):Play()
					end
				end
				--hello, this is open source. For some reason roblox don't show frame of button so i fixed it. Tried to do it in button creating code but it didn't work. Am I dumb?
				for i,v in pairs(tabf:GetChildren()) do
					if v:IsA("Frame") then
						v.ZIndex = 99999
					end
				end
				game:GetService("TweenService"):Create(Tabu,TweenInfo.new(0.2),{BackgroundTransparency = 0.9}):Play()
				tabf.Parent = Elements
			end
		end)
