			InfoIcon.BorderSizePixel = 0
			InfoIcon.Position = UDim2.new(0.936090231, 0, 0.200000018, 0)
			InfoIcon.Size = UDim2.new(0.0639097765, 0, 0.666666687, 0)
			InfoIcon.ZIndex = 55
			InfoIcon.Image = "rbxassetid://13012250972"
			InfoIcon.ImageTransparency = 0.500
			UIAspectRatioConstraint_2.Name = guid()
			UIAspectRatioConstraint_2.Parent = InfoIcon
			toggled = currentval
			local function JALYZQ_script()
				local script = Instance.new('LocalScript', ToggleIcon)
				local TS = game:GetService("TweenService")
				script.Parent.InputBegan:Connect(function(input)
					if input.UserInputType == Enum.UserInputType.MouseButton1 then
						if toggled then
							TS:Create(ToggleIcon,TweenInfo.new(0.2),{ImageColor3 = Color3.fromRGB(255, 255, 0)}):Play()
						else
							TS:Create(ToggleIcon,TweenInfo.new(0.2),{ImageColor3 = Color3.fromRGB(255, 0, 0)}):Play()
						end
					end
				end)
			end
			coroutine.wrap(JALYZQ_script)()
			connection2 = ToggleIcon.InputBegan:Connect(function(i)
				if i.UserInputType == Enum.UserInputType.MouseButton1 then
					if toggled then
						toggled = false
					else
						toggled = true
					end
					return callback(toggled)
				end
			end)
			connection1 = InfoIcon.InputBegan:Connect(function(i)
				if i.UserInputType == Enum.UserInputType.MouseButton1 then
					Library:notification("Toggle Info",desc,5)
				end
			end)
			TGF = {}
			function TGF:ChangeName(text)
				TextLabel.Text = text
			end
			function TGF:ChangeDescription(descc)
				desc = descc
			end
			function TGF:ChangeValue(val)
				callback(val)
				if val then
					TS:Create(ToggleIcon,TweenInfo.new(0.2),{ImageColor3 = Color3.fromRGB(255, 255, 0)}):Play()
				else
					TS:Create(ToggleIcon,TweenInfo.new(0.2),{ImageColor3 = Color3.fromRGB(255, 0, 0)}):Play()
				end
			end
			function TGF:ChangeCallback(func)
				callback = func
			end
			function TGF:Break()
				connection1:Disconnect()
				connection2:Disconnect()
				Toggle:Destroy()
			end
			table.insert(DisconnectButtons,TGF)
			return TGF
		end
